{
  "name": "trading-manager",
  "version": "2.1.0",
  "description": "Trading manager",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others \"yarn start:server\" \"yarn start:client\"",
    "start:server": "cd server && yarn start",
    "start:client": "cd client && yarn start",
    "install:server": "cd server && yarn install",
    "install:client": "cd client && yarn install",
    "install:all": "yarn install && yarn run install:server && yarn run install:client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "trading"
  ],
  "author": "David Daupeyroux <david.ddcreation@gmail.com> (https://github.com/ddcreation)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddcreation/trading-manager"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "private": false
}
